<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.firstandroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/> 
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Start"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
               
         <activity
            android:name="example.firstandroid.Web"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="example.firstandroid.WEB" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
         
          <activity
            android:name="example.firstandroid.MenuP"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="example.firstandroid.MENUP" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
          
          
        
         <activity
            android:name="example.firstandroid.PackageList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="example.firstandroid.PACKAGELIST" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        
    
        
       
      
     
            
      
        
        
       
        
     
        
        
        
        
    
        
        
        
        
        
        
      
      
        
        
       
       
         
         
        
        
         
    </application>

</manifest>